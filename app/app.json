{
  "expo": {
    "name": "garage",
    "slug": "garage",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.elliotnash.garage"
    },
    "scheme": "org.elliotnash.garage",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "org.elliotnash.garage"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": ["assets/**/*"],
    "plugins": [
      "expo-router",
      [
        "expo-asset",
        {
          "extensions": ["glb", "gltf"]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
          }
        }
      ]
    ]
  }
}
